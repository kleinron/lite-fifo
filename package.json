{
  "name": "lite-fifo",
  "version": "1.0.1",
  "license": "MIT",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "files": [
    "src/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/kleinron/lite-fifo.git#readme",
  "description": "Lightweight, optimized, and efficient implementations for FIFO (queue) data structure",
  "devDependencies": {
    "mocha": "^11.7.1",
    "seedrandom": "^3.0.5",
    "semistandard": "^17.0.0",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "lint": "semistandard",
    "lint-with-fix": "semistandard --fix",
    "test": "mocha test/ --recursive --check-leaks --require test/src/setup.js",
    "test-stress": "export STRESS_TESTS_ENABLED=true && export STRESS_TESTS_COUNT=10000 && mocha test/ --recursive --check-leaks --require test/src/setup.js",
    "types": "node ./node_modules/typescript/bin/tsc",
    "ci": "npm run lint && npm run test && npm run types",
    "prepack": "npm run ci && npm run test-stress",
    "prepublishOnly": "npm run ci && npm pack --dry-run",
    "publish-check": "npm run ci && npm pack --dry-run && echo 'âœ… Ready to publish!'",
    "release": "npm run ci && npm publish"
  },
  "author": {
    "name": "Ron Klein",
    "email": "npm@ronklein.co.il",
    "url": "https://ronklein.co.il"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kleinron/lite-fifo.git"
  },
  "keywords": [
    "queue",
    "fifo",
    "lite",
    "lightweight",
    "array",
    "data-structure",
    "data structure",
    "zero-dependency"
  ],
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
